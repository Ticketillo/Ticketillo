{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prebuild": "rimraf build",
        "start": "craco start",
        "build": "craco build",
        "eject": "craco eject",
        "generate": "npm run generate:openapi && npm run generate:icons && npm run generate:images",
        "generate:icons": "node script/iconGenerator.js src/assets/icons/ src/icons/",
        "generate:images": "node script/imagesGenerator.js src/assets/images/",
        "generate:openapi:services": "openapi -i ../backend/openapi-spec.json -o src/api/service --exportSchemas=true --exportModels=false --useUnionTypes --postfix=Api",
        "generate:openapi:models": "openapi -i ../backend/openapi-spec.json -o src/models/api --exportSchemas=false --useUnionTypes  --exportServices=false --exportCore=false",
        "generate:openapi": "yarn generate:openapi:services && yarn generate:openapi:models && node script/openapiPostScript.js",
        "lint": "eslint . --ext .ts,.json --max-warnings=0"
    },
    "dependencies": {
        "@craco/craco": "^7.0.0",
        "@radix-ui/react-avatar": "^1.0.2",
        "@radix-ui/react-slot": "^1.0.1",
        "@radix-ui/react-context-menu": "^2.1.3",
        "@radix-ui/react-toast": "^1.1.3",
        "@radix-ui/react-tooltip": "^1.0.5",
        "@tanstack/react-query": "^4.28.0",
        "class-variance-authority": "^0.6.0",
        "clsx": "^1.2.1",
        "lucide-react": "^0.216.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "6.8.0",
        "react-scripts": "^5.0.1",
        "tailwind-merge": "^1.12.0",
        "tailwindcss": "^3.3.2",
        "tailwindcss-animate": "^1.0.5",
        "typescript": "^4.7.4",
        "zustand": "^4.3.2"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.18.6",
        "@babel/plugin-transform-runtime": "7.19.6",
        "@babel/preset-env": "^7.18.6",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "7.18.6",
        "@types/node": "^20.1.3",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/react-router-dom": "^5.3.2",
        "babel-plugin-named-exports-order": "^0.0.2",
        "customize-cra": "^1.0.0",
        "customize-cra-react-refresh": "^1.1.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "openapi-typescript": "^6.1.0",
        "openapi-typescript-codegen": "^0.23.0",
        "prettier": "^2.8.3",
        "react-error-overlay": "6.0.11",
        "webpack": "^5.75.0"
    },
    "resolutions": {
        "@types/react": "^18.0.27"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
